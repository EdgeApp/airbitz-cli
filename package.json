{
  "name": "airbitz-cli",
  "version": "0.4.4",
  "description": "Airbitz CLI tool",
  "homepage": "https://airbitz.co",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-cli.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "main": "lib/airbitz-cli.cjs.js",
  "module": "lib/airbitz-cli.js",
  "bin": "bin/airbitz-cli",
  "files": [
    "bin/*",
    "CHANGELOG.md",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "fix": "npm run lint -- --fix",
    "lint": "eslint --ext .js,.ts .",
    "precommit": "lint-staged && npm run build",
    "prepare": "husky install && npm run build"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "resolutions": {
    "shell-quote": "1.6.1"
  },
  "dependencies": {
    "base-x": "^3.0.8",
    "chalk": "^1.1.3",
    "edge-core-js": "^0.17.9",
    "edge-exchange-plugins": "^0.11.4",
    "hash.js": "^1.1.7",
    "lib-cmdparse": "^0.1.0",
    "node-getopt": "^0.2.3",
    "rfc4648": "^1.3.0",
    "source-map-support": "^0.4.14",
    "xdg-basedir": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-typescript": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-transform-fake-error-class": "^0.1.0",
    "eslint": ">=6.2.2",
    "eslint-config-standard-kit": ">=0.14.4",
    "eslint-plugin-import": ">=2.18.0",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-prettier": ">=3.0.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-simple-import-sort": ">=4.0.0",
    "eslint-plugin-standard": ">=4.0.0",
    "husky": "^7.0.0",
    "lint-staged": ">=9.0.0",
    "prettier": ">=1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "typescript": "^3.9.6"
  }
}
