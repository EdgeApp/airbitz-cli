{
  "name": "airbitz-cli",
  "version": "0.4.5",
  "description": "Airbitz CLI tool",
  "homepage": "https://airbitz.co",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/airbitz-cli.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Airbitz, Inc.",
  "contributors": [
    "Paul Puey <paul@airbitz.co>",
    "William Swanson <william@airbitz.co>",
    "Tim Horton <tim@airbitz.co>"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "lib/airbitz-cli.js",
  "types": "./lib/src/index.d.ts",
  "bin": "bin/airbitz-cli",
  "files": [
    "bin/*",
    "CHANGELOG.md",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf bin lib",
    "build": "rollup -c",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "precommit": "lint-staged && npm-run-all types",
    "prepare": "husky install && npm-run-all clean -p build types",
    "types": "tsc",
    "verify": "npm-run-all lint build types"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "resolutions": {
    "shell-quote": "1.6.1"
  },
  "dependencies": {
    "base-x": "^3.0.8",
    "chalk": "^1.1.3",
    "edge-core-js": "^0.17.9",
    "edge-exchange-plugins": "^0.11.4",
    "hash.js": "^1.1.7",
    "lib-cmdparse": "^0.1.0",
    "node-getopt": "^0.2.3",
    "rfc4648": "^1.3.0",
    "source-map-support": "^0.4.14",
    "xdg-basedir": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-plugin-transform-fake-error-class": "^1.0.0",
    "eslint": "^7.14.0",
    "eslint-config-standard-kit": "0.15.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.21.0",
    "rollup-plugin-mjs-entry": "^0.1.1",
    "typescript": "^4.1.2"
  }
}
